[run]
omit =
    # omit Python packages
    */.local/*
    /usr/*
    */setup.py
    *.egg/*
    */site-packages/*
    */distutils/*
    
    # Omit non-module scripts
    ./test/*
    ./examples/*

[report]
exclude_lines =
    pragma: no cover
    raise RuntimeError
    raise NotImplementedError
    msg.fatal
    msg.error
